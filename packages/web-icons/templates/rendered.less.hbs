{{#each svgList as |svg name|}}
  .svg_{{name}}{
  width: {{multiply svg.width 0.667}}px; height: {{multiply svg.height 0.667}}px;
  @svg: escape('{{{replace svg.path 'class="ridi_icon"' 'xmlns="http://www.w3.org/2000/svg" fill="#41474b"'}}}');
  display: inline-block; text-indent: -444px; font-size: 0; overflow: hidden;
  background: url('data:image/svg+xml,@{svg}') center center no-repeat;
  background-size: 100% 100%;
  }
{{/each}}
