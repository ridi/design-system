{
  "name": "@ridi/web-icons",
  "version": "0.2.0",
  "description": "RIDI Web Icons",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ridi/design-system.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "files": [
    "svgs",
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "clean": "git clean -xdf --exclude=node_modules .",
    "build": "node build.js",
    "watch": "chokidar 'svgs/*.svg' -c 'node build.js' --initial --silent",
    "svgo": "svgo --config=.svgo.yml --folder=svgs --multipass",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "chalk": "^2.4.1",
    "chokidar-cli": "^1.2.1",
    "lint-staged": "^8.1.0",
    "svgo": "^1.1.1"
  }
}
